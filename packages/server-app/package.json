{
   "name": "server-app",
   "version": "0.0.1",
   "description": "Awesome project developed with TypeORM.",
   "private": true,
   "main": "src/index.ts",
   "engines": {
      "node": ">=8.0.0"
   },
   "devDependencies": {
      "ts-node": "10.7.0",
      "@types/node": "^16.11.10",
      "typescript": "4.5.2",
      "@types/faker": "^6.6.9"
   },
   "dependencies": {
      "express": "^4.18.2",
      "typescript": "5.1.3",
      "uuid": "9.0.0",
      "typeorm": "0.3.17",
      "typeorm-seeding": "1.6.1",
      "debug": "4.3.4",
      "morgan": "~1.10.0",
      "ts-node": "^10.9.1",
      "reflect-metadata": "^0.1.13",
      "sqlite3": "^5.0.2",
      "body-parser": "^1.19.1",
      "class-validator": "^0.14.0",
      "class-validator-jsonschema": "5.0.0",
      "faker": "^4.1.0",
      "chalk": "^5.2.0",
      "commander": "^11.0.0",
      "path": "^0.12.7"
   },
   "scripts": {
      "typeorm": "typeorm-ts-node-esm",
      "start": "ts-node src/index.ts",
      "build": "rm -rf build && tsc",
      "schema:drop": "ts-node --project ./tsconfig.json ./../../node_modules/typeorm/cli.js schema:drop",
      "schema:sync": "ts-node --project ./tsconfig.json ./../../node_modules/typeorm/cli.js schema:sync",
      "seed:config": "ts-node --project ./tsconfig.json ./../../node_modules/typeorm-seeding/dist/cli.js config",
      "seed:run": "ts-node --project ./tsconfig.json ./../../node_modules/typeorm-seeding/dist/cli.js seed",
      "db:setup": "npm run schema:drop && npm run schema:sync && npm run seed:config && npm run seed:run"
   }
}
